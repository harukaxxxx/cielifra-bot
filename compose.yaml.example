version: '1'
services:
  cielifra-bot:
    container_name: cielifra-bot
    build:
      context: .
      dockerfile: Dockerfile
    environment:
      - DISCORD_TOKEN=
      - OWNER_IDS=
      - IMP_TRIGGER_REACTION = "ℹ️"
      - IMP_REJECT_REACTION = "❎"
    volumes:
      - /volume1/docker/cielifra-bot:/base
    restart: unless-stopped
